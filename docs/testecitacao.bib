@article{kawasakiPresenceSharedLeader2017,
	title = {Presence of a {Shared} 5′-{Leader} {Sequence} in {Ancestral} {Human} and {Mammalian} {Retroviruses} and {Its} {Transduction} into {Feline} {Leukemia} {Virus}},
