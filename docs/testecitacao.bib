@article{fiuzaIntradermalImmunizationLeishmania2016,
	title = {Intradermal {Immunization} of {Leishmania} donovani {Centrin} {Knock}-{Out} {Parasites} in {Combination} with {Salivary} {Protein} {LJM19} from {Sand} {Fly} {Vector} {Induces} a {Durable} {Protective} {Immune} {Response} in {Hamsters}},
	volume = {10},
	issn = {1935-2735},
	url = {https://dx.plos.org/10.1371/journal.pntd.0004322},
	doi = {10.1371/journal.pntd.0004322},
	abstract = {Background Visceral leishmaniasis (VL) is a neglected tropical disease and is fatal if untreated. There is no vaccine available against leishmaniasis. The majority of patients with cutaneous leishmaniasis (CL) or VL develop a long-term protective immunity after cure from infection, which indicates that development of an effective vaccine against leishmaniasis is possible. Such protection may also be achieved by immunization with live attenuated parasites that do not cause disease. We have previously reported a protective response in mice, hamsters and dogs with Leishmania donovani centrin gene knock-out parasites (LdCen-/-), a live attenuated parasite with a cell division specific centrin1 gene deletion. In this study we have explored the effects of salivary protein LJM19 as an adjuvant and intradermal (ID) route of immunization on the efficacy of LdCen-/- parasites as a vaccine against virulent L. donovani.},
	language = {en},
	number = {1},
	urldate = {2021-10-16},
	journal = {PLOS Neglected Tropical Diseases},
	author = {Fiuza, Jacqueline Araújo and Dey, Ranadhir and Davenport, Dwann and Abdeladhim, Maha and Meneses, Claudio and Oliveira, Fabiano and Kamhawi, Shaden and Valenzuela, Jesus G. and Gannavaram, Sreenivas and Nakhasi, Hira L.},
	editor = {Ghedin, Elodie},
	month = jan,
	year = {2016},
	pages = {e0004322},
	file = {Fiuza et al. - 2016 - Intradermal Immunization of Leishmania donovani Ce.pdf:C\:\\Users\\ICTPSAIFR4\\Zotero\\storage\\ASYW8HVD\\Fiuza et al. - 2016 - Intradermal Immunization of Leishmania donovani Ce.pdf:application/pdf},
