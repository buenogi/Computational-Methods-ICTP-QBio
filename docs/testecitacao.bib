	pages = {136--143},
	file = {Tandon et al. - 2008 - Association between endogenous feline leukemia vir.pdf:C\:\\Users\\ICTPSAIFR4\\Zotero\\storage\\KP3NAXZP\\Tandon et al. - 2008 - Association between endogenous feline leukemia vir.pdf:application/pdf},
}

