	month = jul,
	year = {2015},
	pmid = {26032495},
	keywords = {Animals, Amino Acid Sequence, Cell Line, Tumor, Cell Nucleus, Cytoplasm, Fluorescence correlation spectroscopy, Fluorescence recovery after photobleaching, Fluorescence Recovery After Photobleaching, Green fluorescent protein, Green Fluorescent Proteins, Mice, Molecular Sequence Data, Nuclear localization signal, Nuclear Localization Signals, Nucleolus, RNA, Spectrometry, Fluorescence},
	pages = {401--406},
	file = {Texto completo:C\:\\Users\\ICTPSAIFR4\\Zotero\\storage\\VANAGTPS\\Kitamura et al. - 2015 - Efficient and dynamic nuclear localization of gree.pdf:application/pdf},
