	year = {2018},
	pages = {141--150},
	file = {Sochorová et al. - 2018 - Evolutionary trends in animal ribosomal DNA loci .pdf:C\:\\Users\\ICTPSAIFR4\\Zotero\\storage\\GG3T3P2S\\Sochorová et al. - 2018 - Evolutionary trends in animal ribosomal DNA loci .pdf:application/pdf},
