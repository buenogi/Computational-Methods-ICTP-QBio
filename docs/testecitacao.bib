	title = {Discovery of a {New} {Endogenous} {Type} {C} {Retrovirus} ({FcEV}) in {Cats}: {Evidence} for {RD}-114 {Being} an {FcEVGag}-{Pol}/{Baboon} {Endogenous} {Virus} {BaEVEnv} {Recombinant}},
