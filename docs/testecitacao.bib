@article{bandeMolecularDetectionPhylogenetic2014,
	title = {Molecular {Detection}, {Phylogenetic} {Analysis}, and {Identification} of {Transcription} {Motifs} in {Feline} {Leukemia} {Virus} from {Naturally} {Infected} {Cats} in {Malaysia}},
